public class ControleEstoque {
    public static void main(String[] args) 
    {
        
        int codigoProduto= 1;
        String nomeProduto;
        String descricaoProduto;
        double precoProduto = 20;
        String categoria;
        double quantidadeProduto = 155;
        double quantidadeVendida = 55;
        double saldoEstoque = quantidadeProduto-quantidadeVendida;
        
        nomeProduto= "Leite Vaca";
        descricaoProduto="Leite Rico em Vitaminas";
        categoria= "Alimento";
        
        System.out.println("Nome"  +  nomeProduto);
        System.out.println("Descricao"  +  descricaoProduto);
        System.out.println("Preco"  +  precoProduto);
        System.out.println("Categoria "+ categoria);
        System.out.println("Quantidade" + quantidadeProduto);
        System.out.println("Quantidade Vendida" + quantidadeVendida);
        System.out.println("Saldo Estoque" + saldoEstoque);
        
        
    
        
        
    }
}
