public class ControleEstoque {
    public static void main(String[] args) {
     
        String NomeProduto;
        double PrecoProduto= 15;
        double QuantidadeProduto= 3;
        
        NomeProduto ="Tablet";
        PrecoProduto = 100;
       
        String MarcaProduto;
        MarcaProduto="Lenovo";

        String NomeProduto1;
        double PrecoProduto1 = 29.99;
        double QuantidadeProduto1= 3;
        
        String MarcaProduto1;
        MarcaProduto1="Sansunmg";
        
        NomeProduto1="Fone de Ouvido";
        
        System.out.println(" Dados do Produto");
        System.out.println("Nome: " + NomeProduto);
        System.out.println("Marca: " + MarcaProduto);
        System.out.println("Preco: $ " + PrecoProduto);
        System.out.println("Quantidade em estoque: " + QuantidadeProduto);
        System.out.println("Nome: " + NomeProduto1);
        System.out.println("Preco: $ " + PrecoProduto1);
        System.out.println("Quantidade em estoque: " + QuantidadeProduto1);
         System.out.println("Marca: " + MarcaProduto1);
        
        
    }
}
